<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
    <display-name>FunctionalCuration</display-name>

    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contact</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Contact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>About</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.About</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Db</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.DB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileTransfer</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.FileTransfer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Me</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Me</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Entity</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.EntityView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewExperiment</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.NewExperiment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Compare</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Compare</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Batch</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Batch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Fitting</servlet-name>
        <servlet-class>uk.ac.ox.cs.chaste.fc.web.Fitting</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contact</servlet-name>
        <url-pattern>/contact.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>About</servlet-name>
        <url-pattern>/about.html</url-pattern>
        <url-pattern>/for_authors.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Db</servlet-name>
        <url-pattern>/db.html</url-pattern>
        <url-pattern>/db/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileTransfer</servlet-name>
        <url-pattern>/upload.html</url-pattern>
        <url-pattern>/submitExperiment.html</url-pattern>
        <url-pattern>/download/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewExperiment</servlet-name>
        <url-pattern>/newexperiment.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Me</servlet-name>
        <url-pattern>/myaccount.html</url-pattern>
        <url-pattern>/myfiles.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Entity</servlet-name>
        <url-pattern>/model/*</url-pattern>
        <url-pattern>/protocol/*</url-pattern>
        <url-pattern>/experiment/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Compare</servlet-name>
        <url-pattern>/compare/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/admin.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Batch</servlet-name>
        <url-pattern>/batch/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Fitting</servlet-name>
        <url-pattern>/fitting.html</url-pattern>
        <url-pattern>/fitting/*</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
</web-app>
